<odoo>
    
    <record id="saldoapp7.formato_papel_1" model="report.paperformat" >
        <field name="name">Formato de Papel 1</field>
        <field name="format">custom</field>
        <field name="orientation">Portrait</field>
        <field name="default" eval="False" />
        <field name="dpi" eval="90" />
        <field name="margin_top" eval="40" />
        <field name="margin_right" eval="7" />
        <field name="margin_bottom" eval="20" />
        <field name="margin_left" eval="7" />
        <field name="header_line" eval="False" />
        <field name="header_spacing" eval="35" />
        <field name="page_width" eval="250" />
        <field name="page_height" eval="250" />
        <field name="report_ids" eval="[(4, ref('saldoapp7.sa_report_movimiento_partner')),
                                        (4, ref('saldoapp7.sa_report_movimiento_user'))]" />
    </record>
    
</odoo>